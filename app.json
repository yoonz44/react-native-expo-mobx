{
  "expo": {
    "name": "react-native-expo-mobx",
    "slug": "react-native-expo-mobx",
    "version": "1.0.0",
    "scheme": "talkpic",
    "orientation": "portrait",
    "icon": "./app/assets/images/logo.png",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./app/assets/images/favicon.png"
    }
  }
}
